source ~/.dotfiles/source/.shell/bak
source ~/.dotfiles/source/.shell/adapt

export DISPLAY=:1

while [ -e /tmp/changeip_pause ]
do
    sleep 1
done

xdotool key 1 ; shcw changeip.1
xdotool key 2 ; shcw changeip.2
xdotool key 3 ; shcw changeip.3
xdotool key 4 ; shcw changeip.4
xdotool key 5 ; shcw changeip.5
