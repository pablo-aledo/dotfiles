export DISPLAY=:1
source ~/.dotfiles/source/.shell/xdotool

touch /tmp/lock

sleep 5

mouseover 50 10000 ~/.dotfiles/shortcuts/images/print_pdf.png
xdotool click 1

mouseover 50 10000 ~/.dotfiles/shortcuts/images/print_download.png
xdotool click 1
sleep 5

xdotool key Ctrl+a
xdotool key Ctrl+c
sleep 1

file=$(xclip -sel clip -o | sed 's/ ([1-3])\././g')
echo $file > /tmp/file
#[ -e ~"/Descargas/$file" ] && xdotool mousemove 930 670 && xdotool click 1
#[ ! -e ~"/Descargas/$file" ] && xdotool key Return
xdotool key Return


rm -rf /tmp/lock

