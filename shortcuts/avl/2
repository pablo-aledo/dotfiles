export DISPLAY=:1

title=$( cat /tmp/avl_line | sed 's/\(.*\) -> \(.*\)/\1/g' )
link=$( cat /tmp/avl_line | sed 's/\(.*\) -> \(.*\)/\2/g' )
echo $title > /tmp/title

[ -x /tmp/avl_chrome_cmd ] ||          google-chrome "$link" >/dev/null 2>/dev/null &
[ -x /tmp/avl_chrome_cmd ] && /tmp/avl_chrome_cmd "$link" >/dev/null 2>/dev/null &
sleep 3

