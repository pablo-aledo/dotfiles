export DISPLAY=:1

title=$( cat /tmp/avl_line | sed 's/\(.*\) -> \(.*\)/\1/g' )
link=$( cat /tmp/avl_line | sed 's/\(.*\) -> \(.*\)/\2/g' | sed 's/avxhm.se/xsava.xyz/g' )
echo $title > /tmp/title

google-chrome "$link" >/dev/null 2>/dev/null &
sleep 3

