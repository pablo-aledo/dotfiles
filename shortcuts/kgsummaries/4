export DISPLAY=:1

rm -fr ~/Descargas/kgsummary.html ~/Descargas/kgsummary_files

xdotool key Ctrl+s
sleep 1
xdotool key Alt+f
sleep 1
xdotool type kgsummary
xdotool key Return
sleep 5

title="$(cat /tmp/kg_title)"
filename="/tmp/kgsummaries/$(echo $title | md5sum | awk '{print $1}')"
htmlq -f ~/Descargas/kgsummary.html '#summary' -t | grep -v '^ *$' | grep '      Summary' -B1000 | grep -v '      Summary' >> $filename
echo '=====' >> $filename
