export DISPLAY=:1

xdotool mousemove 558 460
xdotool click 1
xdotool click 1
xdotool click 1
sleep 1
xdotool key Ctrl+c
xclip -sel clip -o > /tmp/file
mkdir -p "$HOME/learning/metadata/$(cat /tmp/file)"
echo "$(cat /tmp/title)" > "$HOME/learning/metadata/$(cat /tmp/file)/title"

xdotool mousemove 558 540
xdotool click 1
sleep 1
xdotool key Return







