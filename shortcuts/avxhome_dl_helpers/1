export DISPLAY=:1
source ~/.dotfiles/source/adapt
adapt_chromebook

xrandr --output VNC-0 --mode 1280x800
sleep 1
xdotool key Alt+1
xdotool key Alt+d
killall google-chrome
sleep 1
google-chrome mail.google.com &
sleep 20
xdotool mousemove 290 110
xdotool click 1
xdotool type 'is: starred avxhm.se'
xdotool key Return
sleep 5
xdotool mousemove 290 200
xdotool click 1
