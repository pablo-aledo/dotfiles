export DISPLAY=:1

xdotool mousemove 280 220
xdotool click 1
xdotool click 1
xdotool click 1
xdotool key Ctrl+c
sleep 1

xdotool key Ctrl+t
xdotool key Ctrl+l
sleep 1
xdotool type 'avxhm.se'
xdotool key Return
sleep 5
xdotool mousemove 250 330
xdotool click 1
xdotool key Ctrl+v

for a in $(seq 1 14)
do
    xdotool key BackSpace
    sleep 0.1
done

xdotool key Return
sleep 5

xdotool mousemove 150 300
xdotool click 1
