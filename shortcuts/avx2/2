export DISPLAY=:1

xdotool mousemove 280 220
xdotool click 1
xdotool click 1
xdotool click 1
xdotool key Ctrl+c
sleep 1

xdotool key Ctrl+t
xdotool key Ctrl+l
sleep 1
xdotool type 'avxhm.se'
xdotool key Return
sleep 5
xdotool mousemove 440 150
xdotool click 1
xdotool key Ctrl+v

for a in $(seq 1 14)
do
    xdotool key BackSpace
    sleep 0.1
done

xdotool key Return
xdotool key Return
sleep 1
xdotool mousemove 1150 150
xdotool click 1
xdotool click 1
sleep 5

xdotool key Ctrl+f
xdotool type 'Details'
sleep 1
xdotool mousemove 1150 100
xdotool click 1
xdotool key Return
