snippet tikz
	\documentclass{article}
	\usepackage{tikz}
	\usetikzlibrary{shapes,arrows,fit,decorations.pathreplacing,shadows.blur}
	\tikzset{>=latex}

	\makeatletter
	\pgfdeclareshape{document}{
	\inheritsavedanchors[from=rectangle] % this is nearly a rectangle
	\inheritanchorborder[from=rectangle]
	\inheritanchor[from=rectangle]{center}
	\inheritanchor[from=rectangle]{north}
	\inheritanchor[from=rectangle]{south}
	\inheritanchor[from=rectangle]{west}
	\inheritanchor[from=rectangle]{east}
	% ... and possibly more
	\backgroundpath{
	% store lower left in xa/ya and upper right in xb/yb
	\southwest \pgf@xa=\pgf@x \pgf@ya=\pgf@y
	\northeast \pgf@xb=\pgf@x \pgf@yb=\pgf@y
	% compute corner of "flipped page"
	\pgf@xc=\pgf@xb \advance\pgf@xc by-7.5pt % this should be a parameter
	\pgf@yc=\pgf@yb \advance\pgf@yc by-7.5pt
	% construct main path
	\pgfpathmoveto{\pgfpoint{\pgf@xa}{\pgf@ya}}
	\pgfpathlineto{\pgfpoint{\pgf@xa}{\pgf@yb}}
	\pgfpathlineto{\pgfpoint{\pgf@xc}{\pgf@yb}}
	\pgfpathlineto{\pgfpoint{\pgf@xb}{\pgf@yc}}
	\pgfpathlineto{\pgfpoint{\pgf@xb}{\pgf@ya}}
	\pgfpathclose
	% add little corner
	\pgfpathmoveto{\pgfpoint{\pgf@xc}{\pgf@yb}}
	\pgfpathlineto{\pgfpoint{\pgf@xc}{\pgf@yc}}
	\pgfpathlineto{\pgfpoint{\pgf@xb}{\pgf@yc}}
	\pgfpathlineto{\pgfpoint{\pgf@xc}{\pgf@yc}}
	}
	}
	\makeatother

	\newcommand\compute[1]{\pgfmathsetmacro\result{#1}}
	\definecolor{BgGray}{RGB}{234,234,242}

	\begin{document}

	\tikzstyle{block}    = [draw, thick, blur shadow={shadow blur steps=5}, fill=black!5, rectangle, rounded corners]
	\tikzstyle{box}      = [draw, rectangle, rounded corners, dashed]
	\tikzstyle{gradient} = [draw, blue,top color=blue!20, bottom color=blue!5!white,rectangle, rounded corners]
	\tikzstyle{cyl}      = [draw, cylinder, shape border rotate=90, aspect=0.2, fill=orange!10, draw=orange!80]

	\resizebox{10cm}{!}{
	\begin{tikzpicture}[auto, node distance=6em, minimum height=2em, minimum width=6em]

	    \node [block,align=center,anchor=center]                   (center)    {Center};
	    \node [left of=center, block, inner sep=1em] (west)      {West};
	    \node (north) at (90:5em)    {North};
	    \node [right of=center, gradient, outer sep=.5em]            (east)      {East};
	    \node [above of=east, node distance=5em, text width=1.5cm] (northeast) {North-East};
	    \node [above of=west, node distance=5em, text width=1.5cm] (northwest) {North-West};
	    \node [box, fit=(center) (west)]             (box1)      {};
	    \node[
	      below of=center,
	      shape=document,
	      double copy shadow={
		shadow xshift=-0.5ex,
		shadow yshift=-0.5ex
	      },
	      draw,
	      fill=white,
	      scale=0.8
	      ] (south) {South};

	    \compute{tan(10*2)}
	    \node at (3em,8em) {\result};

	    \coordinate [above of=northwest,node distance=0em, yshift=4em] (loop1);
	    \coordinate [above of=northeast,node distance=0em, yshift=4em] (loop2);

	    \draw [->, rounded corners=.2em] (northwest) -- (loop1) -- (loop2) -- (northeast);
	    \draw [->, rounded corners] (south) -| (east);
	    \draw [->] (center.north)+(0em,0.5em) -- ++(0em,2em) -- ++(45:1em);
	    \draw [->, bend right] (west) to (south);
	    \draw [decorate,decoration={brace,amplitude=5pt}] (box1.south east) -- (box1.south west) node [midway] {Go west};
	    \draw (north) .. controls +(right:5em) and +(-90:3em) .. (west);
	    \draw[fill=none, draw=BgGray] (0em,0em) rectangle ++(1em,1em);
	    \draw[transform canvas={xshift=1em}] (north.north west) -- (north.south west);
	\end{tikzpicture}
	}

	\end{document}
