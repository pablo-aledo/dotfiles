snippet lstset
	\definecolor{commentcolor}{rgb}{0,0.6,0}
	\definecolor{typecolor}{rgb}{0,0.4,0.8}
	\definecolor{keywordcolor}{rgb}{0.6,0,0.6}
	\definecolor{numbercolor}{gray}{.25}
	\definecolor{rulecolor}{gray}{.8}
	\definecolor{backgroundcolor}{gray}{0.95}
	\definecolor{stringcolor}{rgb}{2,112,10}
	\definecolor{opercolor}{rgb}{0.6,0.6,0}

	\lstset {
	aboveskip=5pt,
	backgroundcolor=\color{backgroundcolor},            % Choose the background color; you must add \usepackage{color} or \usepackage{xcolor}
	basicstyle=\small\ttfamily,                         % Basic font setting
	belowskip=5pt,
	breakatwhitespace=false,                            % Sets if automatic breaks should only happen at whitespace
	breaklines=true,                                    % Sets automatic line breaking
	captionpos=b,                                       % Sets the caption-position to bottom
	commentstyle=\color{commentcolor},                  
	deletekeywords={...},                               % If you want to delete keywords from the given language
	emph={int,char,double,float,unsigned,void,bool},    % Keywords to emphasize
	emphstyle=\bfseries\color{typecolor},               % Emphasis style
	escapechar=\&,
	escapeinside={(*@}{@*)},                            % If you want to add LaTeX within your code
	extendedchars=true,                                 % Lets you use non-ASCII characters; for 8-bits encodings only, does not work with UTF-8
	firstnumber=1,                                      % First line number
	frame=lrbt,	                                    % Adds a frame around the code
	framerule=1pt,
	framesep=0pt,
	framexbottommargin=5pt,
	framexleftmargin=5pt,
	framextopmargin=5pt,
	keepspaces=true,                                    % Keeps spaces in text, useful for keeping indentation of code (possibly needs columns=flexible)
	keywordstyle=\bfseries\color{keywordcolor},         % Keyword style
	language=C++,                                       % The language of the code
	numberfirstline = true,                             % Number first line
	numbersep=5pt,                                      % How far the line-numbers are from the code
	numbers=none,                                       % Where to put the line-numbers; possible values are (none, left, right)
	numberstyle=\tiny\color{numbercolor},               % The style that is used for the line-numbers
	otherkeywords={*,...},                              % If you want to add more keywords to the set
	rulecolor=\color{rulecolor},                        % If not set, the frame-color may be changed on line-breaks within not-black text (e.g. comments (green here))
	rulesep=.4pt,
	rulesepcolor=\color{rulecolor},
	showspaces=false,                                   % Show spaces everywhere adding particular underscores; it overrides 'showstringspaces'
	showstringspaces=false,                             % Underline spaces within strings only
	showtabs=false,                                     % Show tabs within strings adding particular underscores
	stepnumber=5,                                       % The step between two line-numbers. If it's 1, each line will be numbered
	stringstyle=\ttfamily\color{stringcolor},           % Style for strings
	tabsize=2,	                                    % Sets default tabsize to 2 spaces
	title=\lstname,                                     % Show the filename of files included with \lstinputlisting; also try caption instead of title
	classoffset=1,                                      % Starting new class
	otherkeywords={>,<,.,;,-,!,=,~},                    % More keywords
	morekeywords={>,<,.,;,-,!,=,~},                     % More keywords
	keywordstyle=\bfseries\color{opercolor},            % Style
	classoffset=0,                                      % Ending new class
	}
