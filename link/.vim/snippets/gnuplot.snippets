	set yrange [-8:157]
	set label 1 "\"Picasso's \'Penguin on Beach\'\"" at 64,139 center
	plot 'blutux.rgb' binary array=(128,128) flipy format='%uchar' every 8:8 with rgbimage
	pause -1 "Hit return to continue"
