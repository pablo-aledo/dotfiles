	# Thanks to mrb2j@kelvin.seas.Virginia.EDU for telling us about this.
	# Extreme (Gumbel extreme value) PDF and CDF
	alpha = 1.0; u = 0.0
	mu = u + (0.577215665/alpha)   # Euler's constant
	sigma = pi/(sqrt(6.0)*alpha)
	xmin = mu - 6.0 * sigma
