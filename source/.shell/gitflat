gitflat(){

mkdir ../gitflat/
n='000'
for a in `git log | egrep '^commit' | cut -d" " -f2 | tac`
do
	git checkout $a
	mkdir ../gitflat/$n
	cp -r * ../gitflat/$n/
	n=`printf '%03d' $(($n+1))`
done

}

alias gitconfig='HOME=~/.dotfiles/link_if_new'

gitsquash(){
    echo "change all to squash except the first one (pick)"
    read
    git rebase -i origin/master
}
