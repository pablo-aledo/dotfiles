links(){
	sed -n 's/.*href="\([^"]*\).*/\1/p' $1
	sed -n 's/.*href=\o47\([^\o47]*\).*/\1/p' $1
	grep -Po '(?<=href=")[^"]*' $1
}

wget_full(){
	wget -kcmrnp $1
}

wget_full_2(){
    wget -t5 -kcmrnp --user-agent="Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/92.0.4515.159 Safari/537.36" -e robots=off $1
}
