FOLDER=$(dirname $(readlink -f $0))
#source $FOLDER/pkg
MYSELF=$(readlink -f $0)

unalias which 2>/dev/null

wh(){
	if [ "`cat $MYSELF | grep 'for a in ' | grep $1 `" ]
	then
		unset -f $1 2>/dev/null
		which $1
	else
		which $1
	fi
}

alias which=wh

echo '%name%(){'                                                                         > /tmp/template
echo '   [ "`unset -f %name%; which %name% | grep found`" = "" ] || pkg install %name%' >> /tmp/template
echo '   [ "`unset -f %name%; which %name% | grep found`" = "" ] || return'             >> /tmp/template
echo '   `unset -f %name%; which %name%` $*;'                                           >> /tmp/template
echo '}'                                                                                >> /tmp/template
echo ''                                                                                 >> /tmp/template

for a in meld gvim google-chrome espeak fbreader g++ inkscape skype mksquashfs svn virtualbox vlc w3m wine youtube-dl x11vnc gimp terminator arandr baobab fdupes git jdownloader2 lame mplayer mencoder paman pdftk rar sqlite sqlite3 convert tmux detox ffmpeg dot pdflatex sshfs duplicity ranger terminology wps ncdu pcmanfm htop x2goclient pdfjam pdfcrop pdfnup aria2c cmake pithos aws pip scala sbt ammonite hg qalculate curl vagrant mendeleydesktop canto wkhtmltopdf pandoc icdiff latexrun fzf z numlockx
do
	grep $a'()' $MYSELF >/dev/null && continue
	cat /tmp/template | sed s/%name%/$a/g >> $MYSELF
done

meld(){
   [ "`unset -f meld; which meld | grep found`" = "" ] || pkg install meld
   [ "`unset -f meld; which meld | grep found`" = "" ] || return
   `unset -f meld; which meld` $*;
}

